<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="JSX Harmony" injector-id="xml">
      <display-name>h1</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("h1")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="JSX Harmony" injector-id="xml">
      <display-name>td</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("td")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
  </component>
</application>